    "columns": [
      {"name": "region", "type": "string", "meaning": "Geographical region", "synonyms": ["region", "area"]},
      {"name": "QOQ_Totalpower_Deployed", "type": "numeric", "meaning": "Quarterly total deployed power (MW)", "synonyms": ["total deployed power", "power deployed"]},
      {"name": "QOQ_Availablepower_Deployed", "type": "numeric", "meaning": "Quarterly available deployed power (MW)", "synonyms": ["available power"]},
      {"name": "QOQ_Underconstructionpower", "type": "numeric", "meaning": "Quarterly power under construction (MW)", "synonyms": ["power pipeline"]},
      {"name": "QOQ_Landpower", "type": "numeric", "meaning": "Quarterly land power (MW)", "synonyms": ["land capacity"]},
      {"name": "QOQ_per_totalmarket", "type": "numeric", "meaning": "% of total market power", "synonyms": ["market %"]},
      {"name": "QOQ_per_totalpoweravailable", "type": "numeric", "meaning": "% of available power", "synonyms": ["available %"]},
      {"name": "QOQ_per_underconstructionpower", "type": "numeric", "meaning": "% of under-construction power", "synonyms": ["pipeline %"]},
      {"name": "QOQ_per_landpower", "type": "numeric", "meaning": "% of land power", "synonyms": ["land %"]}],
    "calculatedMetics":[
        {
          "name": "TotalDeployedQoQ",
          "category": "Power",
          "description": "Quarter-over-quarter deployed power (MW).",
          "aliases": ["qoq deployed", "deployed qoq mw", "total deployed qoq"],
          "calculation": "Take the maximum value of QOQ_Totalpower_Deployed, divide by 1000 to convert to MW and PropertyType as colocation,rounded to the nearest units place"
        },
        {
          "name": "AvailableDeployedQoQ",
          "category": "Power",
          "description": "Quarter-over-quarter available deployed power (MW).",
          "aliases": ["available deployed qoq", "available deploy qoq mw", "qoq available power deployed"],
          "calculation": "Take the maximum value of QOQ_Availablepower_Deployed, divide by 1000 to convert to MW and PropertyType as colocation,rounded to the nearest units place"
        },
        {
          "name": "UnderConstructionPowerQoQ",
          "category": "Pipeline",
          "description": "Quarter-over-quarter under construction power (MW).",
          "aliases": ["uc power qoq", "under construction qoq", "pipeline qoq power"],
          "calculation": "Take the maximum value of QOQ_Underconstructionpower, divide by 1000 to convert to MW and PropertyType as colocation,rounded to the nearest units place"
        },
        {
          "name": "LandQoQ",
          "category": "Land",
          "description": "Quarter-over-quarter land bank power (MW).",
          "aliases": ["land qoq", "land mw qoq", "land capacity qoq"],
          "calculation": "Take the maximum value of QOQ_Landpower, divide by 1000 to convert to MW and PropertyType as colocation,rounded to the nearest units place"
        },
        {
          "name": "TotalDeployedQoQPercentage",
          "category": "Percentage",
          "description": "Quarter-over-quarter percentage change for total deployed power.",
          "aliases": ["total deployed qoq percent", "deployed qoq %"],
          "calculation": "Take the maximum value of QOQ_per_totalmarket."
        },
        {
          "name": "AvailableDeployedQoQPercentage",
          "category": "Percentage",
          "description": "Quarter-over-quarter percentage change for available deployed power.",
          "aliases": ["available deployed qoq percent", "available deploy qoq %"],
          "calculation": "Take the maximum value of QOQ_per_totalpoweravailable."
        },
        {
          "name": "UnderConstructionPowerQoQPercentage",
          "category": "Percentage",
          "description": "Quarter-over-quarter percentage change for under construction power.",
          "aliases": ["uc power qoq percent", "under construction qoq %"],
          "calculation": "Take the maximum value of QOQ_per_underconstructionpower."
        },
        {
          "name": "LandQoQPercentage",
          "category": "Percentage",
          "description": "Quarter-over-quarter percentage change for land power.",
          "aliases": ["land qoq percent", "land qoq %"],
          "calculation": "Take the maximum value of QOQ_per_landpower."
        }
    ]
  },
